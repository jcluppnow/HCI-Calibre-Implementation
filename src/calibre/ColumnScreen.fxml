<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="800.0" prefWidth="1200.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="calibre.ColumnScreenController">
    <stylesheets>
        <URL value="@columnscreen.css" />
    </stylesheets>
    
    <fx:define>
            <Image fx:id="PlusIcon" url="@Resources/ColumnIcons/PlusIcon.png" />
    </fx:define>
    <fx:define>
            <Image fx:id="EditIcon" url="@Resources/ColumnIcons/EditIcon.png" />
    </fx:define>
    <fx:define>
            <Image fx:id="JumpArrowIcon" url="@Resources/ColumnIcons/JumpArrowIcon.png" />
    </fx:define>
    <fx:define>
            <Image fx:id="QuestionMarkIcon" url="@Resources/ColumnIcons/QuestionMarkIcon.png" />
    </fx:define>
    <children>
       <Label layoutX="31.0" layoutY="15.0" text="Columns - Add or Manage" textFill="WHITE">
            <font>
                <Font size="42.0" />
            </font>
        </Label>
        
        <Button layoutX="800.0" layoutY="12.0" mnemonicParsing="false" onAction="#handleStub" style="-fx-background-color: AARRGGBB;">
          <graphic>
              <ImageView image="$PlusIcon" fitHeight="50.0" fitWidth="50.0" />
          </graphic>
        </Button>
        
        <Button layoutX="900.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleStub" style="-fx-background-color: AARRGGBB;">
          <graphic>
              <ImageView image="$EditIcon" fitHeight="50.0" fitWidth="50.0" />
          </graphic>
        </Button>
        
        <Button layoutX="1000.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleStub" style="-fx-background-color: AARRGGBB;">
          <graphic>
              <ImageView image="$JumpArrowIcon" fitHeight="50.0" fitWidth="50.0" />
          </graphic>
        </Button>
        
        <Button layoutX="1100.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleStub" style="-fx-background-color: AARRGGBB;">
          <graphic>
              <ImageView image="$QuestionMarkIcon" fitHeight="50.0" fitWidth="50.0" />
          </graphic>
        </Button>
      <TableView layoutX="37.0" layoutY="82.0" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
        
        
    </children>
</AnchorPane>
